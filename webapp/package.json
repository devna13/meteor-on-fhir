{
  "name": "fhir-demo",
  "license": "MIT",
  "version": "1.2.0",
  "repository": "http://github.com/clinical-meteor/meteor-on-fhir",
  "description": "Demo meteor app supporting the FHIR protcol.",
  "scripts": {
    "start": "NODE_ENV=test meteor --settings configs/settings.dev.json",
    "debug": "DEBUG=true meteor --settings configs/settings.dev.json",
    "trace": "DEBUG=true TRACe=true meteor --settings configs/settings.dev.json",
    "test": "meteor test --driver-package dispatch:mocha-phantomjs --port 5000",
    "test-app": "meteor test --full-app --driver-package dispatch:mocha-phantomjs --port 5000 --once",
    "package-verification": "starrynight run-tests --type package-verification",
    "verification": "starrynight run-tests --type verification",
    "validation": "starrynight run-tests --type validation",
    "spacejam": "starrynight run-tests --framework spacejam",
    "staging": "meteor deploy staging.meteor.com --settings configs/settings.staging.json",
    "production": "meteor deploy production.meteor.com --settings configs/settings.prod.json",
    "coverage": "COVERAGE=1 meteor",
    "monitor": "COVERAGE=1 meteor test --driver-package dispatch:mocha --port 5000",
    "nightwatch": "nightwatch -c .meteor/nightwatch.json --tag circle",
    "phantom": "nightwatch -c .meteor/nightwatch.json --env phantom",
    "circle-nightwatch": "nightwatch -c .meteor/nightwatch.circle.json --tag circle",
    "desktop": "meteor-desktop",
    "dockerize": "../scripts/build_docker_compose_file.sh"
  },
  "eslintConfig": {
    "parserOptions": {
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "modules": true
      }
    },
    "plugins": [
      "react",
      "meteor"
    ],
    "extends": [],
    "globals": {
      "server": false,
      "browser": false,
      "expect": false
    },
    "rules": {
      "import/no-unresolved": 0,
      "react/jsx-uses-vars": 1
    }
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "bootstrap": "^3.3.7",
    "classnames": "^2.2.3",
    "d3": "3.5.17",
    "d3-scale": "^1.0.5",
    "faker": "^4.1.0",
    "fhir": "^3.1.0",
    "geolib": "^2.0.24",
    "google-map-react": "^0.25.0",
    "google-maps": "^3.2.1",
    "jquery": "^3.2.1",
    "jquery-validation": "^1.17.0",
    "katex": "^0.9.0-alpha1",
    "lodash": "^4.17.4",
    "material-ui": "^0.19.4",
    "meteor-node-stubs": "~0.2.0",
    "moment": "^2.18.1",
    "node-geocoder": "^3.21.1",
    "normalize.css": "^7.0.0",
    "object-path": "^0.11.4",
    "onecolor": "^3.0.4",
    "opentok": "^2.6.0",
    "opentok-react": "^0.7.0",
    "prop-types": "^15.5.10",
    "radium": "^0.19.6",
    "react": "^15.6.1",
    "react-addons-css-transition-group": "^15.6.2",
    "react-addons-pure-render-mixin": "^15.6.2",
    "react-bootstrap": "^0.31.5",
    "react-d3-basic": "^1.6.11",
    "react-dom": "15.6.1",
    "react-dropzone-component": "^3.0.0",
    "react-easy-chart": "^0.3.0",
    "react-event-timeline": "^1.4.0",
    "react-faux-dom": "^4.1.0",
    "react-fontawesome": "^1.6.1",
    "react-hexgrid": "^1.0.1",
    "react-highcharts": "^15.0.0",
    "react-icons": "^2.2.7",
    "react-katex": "^1.2.0",
    "react-komposer": "^2.0.0",
    "react-material-icons": "^1.0.2",
    "react-mixin": "^4.0.0",
    "react-modal-dialog": "^4.0.7",
    "react-motion": "^0.5.2",
    "react-rotating-text": "^1.2.1",
    "react-router": "^2.8.0",
    "react-router-bootstrap": "^0.23.3",
    "react-router-transition": "^1.1.2",
    "react-scifi": "0.1.0-alpha",
    "react-scroll-box": "^0.2.9",
    "react-slick": "^0.15.4",
    "react-tap-event-plugin": "^2.0.1",
    "react-tappable": "^1.0.2",
    "react-toolbox": "^2.0.0-beta.12",
    "react-visjs-timeline": "1.4.0-beta-1",
    "scrollto-element": "^1.0.4",
    "vis": "^4.20.1",
    "xml2js": "^0.4.19"
  }
}
